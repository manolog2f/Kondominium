@model IEnumerable<Kondominium_Entities.DashClientesMorosos>

<div class="table-responsive">
    <table class="table table-striped table-hover table-bordered" id="dataTable2" width="100%" cellspacing="0">
        <thead>
            <tr>
                @*<th>
                        @Html.DisplayNameFor(model => model.ClienteId)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.FullName)
                    </th>*@
                <th>
                    @Html.DisplayNameFor(model => model.PropiedadId)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.VPropiedad)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SaldoActual)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    @*<td>
                            @Html.DisplayFor(modelItem => item.ClienteId)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.FullName)
                        </td>*@
                    <td>
                        @Html.DisplayFor(modelItem => item.PropiedadId)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.VPropiedad)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.SaldoActual)
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
<div>
    <button onclick="exportTableToExcel('dataTable2')" class="btn btn-info"><i class="fa fa-file-excel" aria-hidden="true"></i> Exportar Excel</button>
</div>